<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://ns.adobe.com/air/application/1.5">
	<id>AirBrowser</id>
	<filename>AirBrowser</filename>
	<name>AirBrowser</name>
	<version>0.1</version>
	<description>A Web Browser</description>
	<copyright>Daniel Bartlett [dan@f-box.org] 01 May 2009</copyright>
	<initialWindow>
		<content>bin/AirBrowser.swf</content>
		<title>AirBrowser</title>
		<systemChrome>standard</systemChrome>
		<transparent>false</transparent>
